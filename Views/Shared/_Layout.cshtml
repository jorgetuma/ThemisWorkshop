<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0" />
    <title>@ViewData["Title"] - ThemisWorkshop</title>

    <link rel="stylesheet" href="~/lib/bootstrap/dist/css/bootstrap.min.css" />
    <link rel="stylesheet" href="~/css/site.css" asp-append-version="true" />
    <link rel="stylesheet" href="~/ThemisWorkshop.styles.css" asp-append-version="true" />

    <style>
        /* Font */

        body {
            color: #323232 !important;
        }

        /* Buttons */

        .btn-positive {
            background-color: #323232;
            color: #FFFFFF !important;
            width: 8rem;
        }

        .btn-positive-twice {
            background-color: #323232;
            color: #FFFFFF !important;
            width: 16rem;
        }

        .btn-positive:hover {
            color: #323232 !important;
        }

        .btn-negative {
            border: none;
            outline-offset: -2px;
            outline: 2px solid #323232;
            color: #323232 !important;
            width: 8rem;
        }

        .btn-negative:hover {
            color: #FFFFFF !important;
        }

        a.btn-disabled {
            pointer-events: none;
            color: #4b4b4b;
        }

        /* Navbar's setting */

        .bg-black-dark {
            background-color: #323232;
            color: #FFF;
        }

        .bg-black-light {
            background-color: #4b4b4b;
            color: #FFF;
        }
        
        a {
            text-decoration: none;
            color: #FFF !important;
        }

        .hover-to-bg-black-light:hover {
            background-color: #FFF !important;
            color: #323232 !important;
            font-weight: bold;
        }

        .navbar-height-3 {
            height: 3rem;
        }

        .navbar-height-4 {
            height: 4rem;
        }

        .fixed-navbar {
            position: fixed;
            top: 0;
            bottom: 0;
        }

        .navbar-shown {
            width: 12rem;
        }

        .navbar-hidden {
            width: 4rem;
        }
            
        .fixed-topbar-shown {
            position: fixed;
            top: 0;
            padding-left: 12rem;
        }

        .fixed-topbar-hidden {
            position: fixed;
            top: 0;
            padding-left: 4rem;
        }

        .content-shown {
            padding-left: 12rem;
            padding-top: 4rem;
        }

        .content-hidden {
            padding-left: 4rem;
            padding-top: 4rem;
        }

        .hidden {
            display: none;
        }

         /* Modal */

        .modal-backdrop {
            display: none;
        }

        /* Pagination */

        .page-link {
            background-color: #323232;
        }

        .page-link:hover {
            background-color: #4b4b4b;
            color: #4b4b4b !important;
        }

        /* Colors */

        .red {
            color: #FF6663 !important;
        }

        .yellow {
            color: #FDFD97 !important;
        }

        .green {
            color: #9EE09E !important;
        }

        .blue {
            color: #9EC1CF !important;
        }
    </style>
</head>
