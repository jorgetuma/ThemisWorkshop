    <script>
        function toggle_navbar() {
            var main_text = document.getElementById("main_title");

            if (main_text.textContent == "Themis Workshop") {
                main_text.textContent = "T W";
            } else {
                main_text.textContent = "Themis Workshop";
            }

            const names = document.querySelectorAll('[id="name"]');
            for (var i = 0; i < names.length; i++) {
                names[i].classList.toggle("hidden");
            }

            const icons = document.querySelectorAll('[id="icon"]');
            for (var i = 0; i < icons.length; i++) {
                icons[i].classList.toggle("justify-content-center");
            }

            var navbar = document.getElementById("navbar");
            navbar.classList.toggle("navbar-shown");
            navbar.classList.toggle("navbar-hidden");

            var topbar = document.getElementById("topbar");
            topbar.classList.toggle("fixed-topbar-shown");
            topbar.classList.toggle("fixed-topbar-hidden");

            var content = document.getElementById("content");
            content.classList.toggle("content-shown");
            content.classList.toggle("content-hidden");

            var isNavbarHidden = navbar.classList.contains("navbar-hidden");
            localStorage.setItem("navbarState", isNavbarHidden);

            var isTopbarHidden = topbar.classList.contains("fixed-topbar-hidden");
            localStorage.setItem("topbarState", isTopbarHidden);

            var isContentHidden = content.classList.contains("content-hidden");
            localStorage.setItem("contentState", isContentHidden);

            var isMainTextShort = main_text.textContent === "T W";
            localStorage.setItem("mainTextState", isMainTextShort);

            var namesHidden = Array.from(names).map(name => name.classList.contains("hidden"));
            localStorage.setItem("namesState", JSON.stringify(namesHidden));

            var iconsCentered = Array.from(icons).map(icon => icon.classList.contains("justify-content-center"));
            localStorage.setItem("iconsState", JSON.stringify(iconsCentered));
        }

        document.addEventListener("DOMContentLoaded", function() {
            var navbarState = localStorage.getItem("navbarState");
            if (navbarState === "true") {
                document.getElementById("navbar").classList.add("navbar-hidden");
                document.getElementById("navbar").classList.remove("navbar-shown");
            }

            var topbarState = localStorage.getItem("topbarState");
            if (topbarState === "true") {
                document.getElementById("topbar").classList.add("fixed-topbar-hidden");
                document.getElementById("topbar").classList.remove("fixed-topbar-shown");
            }

            var contentState = localStorage.getItem("contentState");
            if (contentState === "true") {
                document.getElementById("content").classList.add("content-hidden");
                document.getElementById("content").classList.remove("content-shown");
            }

            var mainTextState = localStorage.getItem("mainTextState");
            if (mainTextState === "true") {
                document.getElementById("main_title").textContent = "T W";
            } else {
                document.getElementById("main_title").textContent = "Themis Workshop";
            }

            var namesState = localStorage.getItem("namesState");
            if (namesState) {
                namesState = JSON.parse(namesState);
                var names = document.querySelectorAll('[id="name"]');
                for (var i = 0; i < names.length; i++) {
                    if (namesState[i] === true) {
                        names[i].classList.add("hidden");
                    } else {
                        names[i].classList.remove("hidden");
                    }
                }
            }

            var iconsState = localStorage.getItem("iconsState");
            if (iconsState) {
                iconsState = JSON.parse(iconsState);
                var icons = document.querySelectorAll('[id="icon"]');
                for (var i = 0; i < icons.length; i++) {
                    if (iconsState[i] === true) {
                        icons[i].classList.add("justify-content-center");
                    } else {
                        icons[i].classList.remove("justify-content-center");
                    }
                }
            }
        });
    </script>

    <script src="~/lib/jquery/dist/jquery.min.js"></script>
    <script src="~/lib/bootstrap/dist/js/bootstrap.bundle.min.js"></script>
    <script src="~/js/site.js" asp-append-version="true"></script>
    @await RenderSectionAsync("Scripts", required: false)
</body>
</html>
